{
    "browser_action": {
        "default_title": "Copy link to TikNot",
        "default_icon": {
            "48": "icons/tiknot48.png"
        }
    },
    "description": "A list of methods you can perform on a window.",
    "homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/window-manipulator",
    "manifest_version": 2,
    "name": "Tiknot",
    "version": "1.0",
    "permissions": ["tabs", "storage", "webNavigation", "contextMenus"],
    "background": {
        "scripts": ["node_modules/webextension-polyfill/dist/browser-polyfill.min.js", "background.js"],
        "persistent": false
    }
}
